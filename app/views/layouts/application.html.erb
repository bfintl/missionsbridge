<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= stylesheet_link_tag 'reset', 'grid', 'application' %>
    <%= render :partial => 'layouts/ie' %>    
    <%= yield(:head) %>
  </head>
  <body>
    <%= yield %>
    <aside>
      <%= yield(:aside) %>
    </aside>
    <footer>
      <%#= javascript_include_tag 'http://ajax.googleapis.com/ajax/libs/jquery/1.3.1/jquery.min.js' %>
      <%= javascript_include_tag 'jquery.js' %>
      <%= yield(:footer) %>
    </footer>
  </body>
</html>